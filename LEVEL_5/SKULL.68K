; MID BOSS - LEVEL 5 - "Giant Skull"

; --------------------------------------------------------------------------

SKULL_SETUP	LEA	OBJ_SKULL_A,A6
		JSR	O_OBJECT_SETUP	 
		BNE.S	.end

		MOVE.L	A1,A2
		JSR	OBJECT_ALLOC
		BNE	.error
		LEA	SKULL_DET_DATA,A5
		JSR	O_OSET
		MOVE.L	A2,OBJ_ADDR6(A1)
		MOVE.L	A1,OBJ_ADDR6(A2)

		MOVE.L	#LOGIC_SKULL,OBJ_LOGIC_DATA(A2)
		MOVE.W	#$65,OBJ_LMSPD(A2)
		MOVE.W	#SFX_ESKULL_A,OBJ_FX0_SFXNUM(A2)
		MOVE.W	#-$400,OBJ_CNT1(A2)
		MOVE.W	#120,OBJ_CNT6(A1)

		MOVEQ	#0,D0
.end		RTS

.error		MOVE.L	A2,A1
		JSR	O_CLEAR_S
		MOVEQ	#1,D0
		RTS

; --------------------------------------------------------------------------

SKULL_CLEAR	JSR	O_CLEAR_S
		MOVE.L	OBJ_ADDR2(A1),A2
		JSR	O_DEALL_S
		JSR	CLEAR_DAMAGE
		JSR	SFX3D_KILL
		MOVE.L	OBJ_ADDR6(A1),A1
		JSR	DETECT_DEALL
		CLR.B	OBJ_FLAG0(A1)
		JSR	SCADD_MIDBOSS
		JMP	ECL_KILL

; --------------------------------------------------------------------------

SKULL_MOVE	BTST	#O_CLEAR,OBJ_FLAG0(A1)
		BNE.S	SKULL_CLEAR
	
		CLR.L	OBJ_XINERTIA(A1)

		JSR	LOGIC_CTRL
		JSR	SFX3D_SPEED

		MOVE.L	OBJ_ADDR6(A1),A6	;Hit update.
		MOVE.W	OBJ_SHOT_AMT(A6),D0
		ADD.W	D0,OBJ_SHOT_AMT(A1)
		MOVE.B	OBJ_FLASH1(A6),D0
		ADD.B	D0,OBJ_FLASH1(A1)

		JSR	DAMAGE_CTRL
		JSR	EDAMAGE_METER

		MOVE.L	OBJ_ADDR6(A1),A6	;Move detect area.
		MOVE.L	OBJ_XPOS(A1),OBJ_XPOS(A6)
		MOVE.W	OBJ_ZPOS(A1),D0
		SUB.W	#8,D0
		MOVE.W	D0,OBJ_ZPOS(A6)
		MOVE.L	OBJ_SPD(A1),OBJ_SPD(A6)
		CLR.W	OBJ_SHOT_AMT(A6)
		CLR.B	OBJ_FLASH1(A6)

		BSR	SKULL_MOUTH

		MOVE.L	OBJ_ADDR5(A1),D1
		BEQ.S	.fchk
		MOVE.L	D1,A2
		ADDQ.L	#2,A2
		CMP.W	#-1,(A2)
		BEQ.S	.fset
		CMP.W	#-2,(A2)
		BNE.S	.fnext
		CLR.L	OBJ_ADDR5(A1)
		BRA.S	.fchk
.fset		MOVE.W	#8,OBJ_CNT5(A1)
		MOVE.L	OBJECT_DATA+OBJ_XPOS,OBJ_ADDR4(A1)
		ADDQ.L	#2,A2
		
		MOVE.W	#SFX_LAZERFIRE_A,D0
		JSR	CSFX_ONESHOT_V

.fnext		MOVE.L	A2,OBJ_ADDR5(A1)

.fchk		TST.W	OBJ_CNT5(A1)
		BEQ.S	.misc
		BTST	#O_DEAD,OBJ_FLAG0(A1)
		BNE.S	.end
		SUBQ.W	#1,OBJ_CNT5(A1)
		BPL	SKULL_BEAM	
	
.misc		SUBQ.W	#1,OBJ_CNT6(A1)
		BPL.S	.end
		CLR.W	OBJ_CNT6(A1)
		CMP.W	#$B00,OBJ_ZPOS(A1)
		BPL.S	.end
		
		MOVE.W	#12+15,OBJ_CNT6(A1)
		BCHG	#0,OBJ_FLAG1(A1)
		BEQ.S	.fr

.fl		BTST	#2,OBJ_FLAG1(A1)
		BNE.S	.end
		MOVE.L	#SKULL_FTAB_L,OBJ_ADDR5(A1)		
		MOVE.L	#-48*$100,OBJ_ADDR3(A1)
		RTS

.fr		BTST	#1,OBJ_FLAG1(A1)
		BNE.S	.end
		MOVE.L	#SKULL_FTAB_R,OBJ_ADDR5(A1)		
		MOVE.L	#48*$100,OBJ_ADDR3(A1)
.end		RTS

