SKULL_FTAB_L	
		DC.W	$20,$20,$00
		DC.W	$20,$20,$00
		DC.W	$20,$00
		DC.W	$20,$20
		DC.W	$40,$20
		DC.W	-1
		DC.W	$40
		DC.W	$40
		DC.W	$40
		DC.W	$40
		DC.W	-2

SKULL_FTAB_R	
		DC.W	$60,$60,$00
		DC.W	$60,$60,$00
		DC.W	$60,$00
		DC.W	$60,$60
		DC.W	$80,$60
		DC.W	-1
		DC.W	$80
		DC.W	$80
		DC.W	$80
		DC.W	$80
		DC.W	-2

; --------------------------------------------------------------------------

SKULL_MOUTH	MOVE.L	OBJ_ADDR7(A1),D1
		MOVE.W	OBJ_CNT0(A1),D0		;Ymove.
		MOVE.W	OBJ_CNT1(A1),D2		;Ystep.
		BPL.S	.forward

.backward	ADD.W	D2,D0
		EXT.L	D0
		ADD.L	D0,D1
		BPL	.set
		MOVEQ	#0,D1
		MOVE.W	D0,D3
		ASR.W	#2,D0
		SUB.W	D3,D0
		CMP.W	#$200,D0
		BPL.S	.spkchk
		SUBQ.W	#1,OBJ_CNT3(A1)
		BPL	.set
		
		MOVE.W	#SFX_SKULLOPEN_A,D0
		JSR	CSFX_ONESHOT_V
		
		MOVE.W	#20,OBJ_CNT4(A1)
		MOVEQ	#0,D2
		MOVE.W	#$400,D0
		LEA	VIS_SKSPLASHD,A0
		BRA	.splash

.spkchk		CMP.W	#$C00,D0
		BMI	.set
		CMP.W	#$80,OBJ_ZPOS(A1)
		BMI.S	.set
		MOVE.W	#5,D7
.spark_lp	MOVEM.L	D0-D2/D7/A1,-(SP)
		LEA	VIS_SKSPARK,A0
		MOVE.L	OBJ_XPOS(A1),D1
		MOVE.L	#-$2000,D2
		JSR	VIS_SET_ZS
		MOVEM.L	(SP)+,D0-D2/D7/A1
		DBRA	D7,.spark_lp
		BRA.S	.set

.forward	ADD.W	D2,D0
		EXT.L	D0
		ADD.L	D0,D1
		CMP.L	#50*$100,D1
		BLT.S	.set
		MOVE.L	#50*$100,D1
		
		SUBQ.W	#1,OBJ_CNT4(A1)
		BPL.S	.set

		MOVE.W	#SFX_SKULLCLOS_A,D0
		JSR	CSFX_ONESHOT_V

		MOVE.W	#40,OBJ_CNT3(A1)
		MOVE.W	#-$400,D2

		LEA	VIS_SKSPLASHU,A0

.splash		MOVEM.L	D0-D2/A1,-(SP)
		MOVE.W	OBJ_ZPOS(A1),D0
		MOVE.L	OBJ_XPOS(A1),D1
		MOVEQ	#0,D2
		JSR	VIS_SET_S
		MOVEM.L	(SP)+,D0-D2/A1

.set		MOVE.W	D0,OBJ_CNT0(A1)
		MOVE.W	D2,OBJ_CNT1(A1)
		MOVE.L	D1,OBJ_ADDR7(A1)
		RTS

VSK_PRN0NC	MOVE.W	D0,D1
		ADD.W	#$100,D1
		JSR	O_PR_A_XIT
		JSR	VIS_PCALC
		MOVE.W	D0,D1
		ADD.W	OBJ_CNT7(A1),D1
		JSR	O_PR_SET0_RS
		MOVE.L	#$01000000,(A6)
		MOVE.L	#$00C00000,20(A6)
		RTS

; --------------------------------------------------------------------------

SKULL_PRINT	JSR	O_CALC_PATCHZ
		JSR	O_PR_ALLOCX
		BRA.L	.misc
		BRA.L	.misc
		BRA.L	.misc
		MOVE.L	OBJ_ADDR0(A1),A0
		MOVE.L	OBJGR_ANM0(A0),A0
		MOVE.L	OBJ_ADDR5(A1),D1
		BEQ.S	.skp
		MOVE.L	D1,A2
		ADD.W	(A2),A0
.skp		JSR	O_FLASH_CHECK_S
		MOVE.L	OBJ_ADDR1(A1),A2
		JSR	O_ALLOC_DEALL_S
		MOVE.L	A0,OBJ_ADDR1(A1)
		JSR	O_PR_SET0

		SUBQ.W	#1,D0
		JSR	O_PR_ALLOCX
		BRA.L	.misc
		BRA.L	.misc
		BRA.L	.misc
		MOVE.L	OBJ_ADDR0(A1),A0
		MOVE.L	OBJGR_ANM1(A0),A0
		JSR	O_FCS_CHK
		MOVE.L	OBJ_ADDR2(A1),A2
		JSR	O_ALLOC_DEALL_S
		MOVE.L	A0,OBJ_ADDR2(A1)
		MOVE.L	OBJ_XPOS(A1),D6
		MOVE.L	OBJ_ADDR7(A1),D7
		JSR	O_PR_SET0_XY
		MULS	RCYFRACT(A5),D5
		ADD.L	RCYPOS(A5),D5
		DIVS	D0,D5
		ADD.W	#97,D5
		CMP.W	20(A6),D5
		BPL.S	.misc
		MOVE.W	D5,20(A6)
.misc		
		BRA	SHOW_DAMAGE

; --------------------------------------------------------------------------

SKULL_DET_HIT	CMP.W	#ID_SHOT,OBJ_ID(A1)
		BEQ	.hit
		CMP.W	#ID_MISS,OBJ_ID(A1)
		BNE.S	.next
.hit		MOVE.L	A0,-(SP)
		MOVE.L	OBJ_ADDR6(A2),A0
		CMP.L	#25*$100,OBJ_ADDR7(A0)
		BMI.S	.closed
		MOVE.L	(SP)+,A0
		JMP	OBJ_HIT_NORM
.closed		MOVE.L	(SP)+,A0
.next		JMP	OBJ_DET_NEXT

SKULL_DET_DATA	DC.W	ID_CAR		;OBJ_ID
		DC.L	SIGN_NEXT	;OBJ_MOVE_CTRL
		DC.L	SIGN_NEXT	;OBJ_PRN_CTRL
		DC.W	2		;OBJ_ZSIZE
		XYRATIO	100,350,100,350
		XYSIZE	27,18
		DC.W	$0000		;OBJ_MASS
		DC.L	SKULL_DET_HIT	;OBJ_HIT_GOTO
		DC.W	$80*40		;OBJ_ZPOS
		DC.L	$100*200	;OBJ_XPOS
		DC.L	$100*0		;OBJ_YPOS

; --------------------------------------------------------------------------

SKULL_BEAM	MOVE.L	OBJ_XPOS(A1),-(SP)
		MOVE.L	OBJ_ADDR3(A1),D0
		ADD.L	D0,OBJ_XPOS(A1)
		LEA	BEAM_SET_DATA,A4
		
		MOVE.L	OBJECT_DATA+OBJ_SPD,D0
		MOVE.W	OBJECT_DATA+OBJ_ZPOS,D1
		MOVE.L	OBJ_ADDR4(A1),D6
		MOVEQ	#0,D7						
		JSR	O_LINE_TO_POS
		
		MOVE.L	(SP)+,OBJ_XPOS(A1)
		RTS

BEAM_PRINT	MOVE.L	OBJ_ADDR0(A1),A0
		MOVE.L	OBJGR_ANM0(A0),A0
		MOVE.W	OBJ_CNT0(A1),D1
		ADD.W	#$20,D1
		CMP.W	#19*$20,D1
		BMI.S	.skp
		MOVE.W	#19*$20,D1
.skp		MOVE.W	D1,OBJ_CNT0(A1)
		AND.W	#$FFE0,D1
		LEA	(A0,D1.W),A0
		JMP	O_T2M_PRINT

BEAM_MOVE	BTST	#O_CLEAR,OBJ_FLAG0(A1)
		BNE	.clear
		JSR	O_RANGE_CHK 	
		BRA.L	.clear
		BRA.L	.clear
		MOVE.L	OBJ_XPOS(A1),D0
		SUB.L	#200*$100,D0
		BPL.S	.pxchk
		NEG.L	D0
.pxchk		CMP.L	#600*$100,D0
		BPL	.clear

		MOVE.W	OBJ_SPD(A1),D0
		MOVE.W	OBJ_ADDR4+2(A1),D1
		LEA	SIGN_DET_ZXKILL,A5	;Sign hit adjust routine.
		JSR	OBJDET_ZXY
		ADD.W	D0,OBJ_ZPOS(A1)
		ADD.L	D1,OBJ_XPOS(A1)

		MOVE.W	DET_HIT_FLAG,D0
		BNE.S	.hit
.cont		MOVE.L	OBJ_ADDR5(A1),D0
		ADD.L	D0,OBJ_SPD(A1)
		MOVE.L	OBJ_ADDR2(A1),D0
		ADD.L	D0,OBJ_YPOS(A1)
		BMI.S	.end
		CMP.L	#50*$100,OBJ_YPOS(A1)
		BPL.S	.clear
.end		RTS

.clear		JMP	O_CLEAR_S

.hit		CMP.W	#OBJ_HIT,D0
		BCS.S	BEAMHIT_OBJECT
		MOVE.L	DET_HIT_DATA1,A2
		CMP.W	#ID_MAIN,OBJ_ID(A2)
		BNE.S	.cont

BEAMHIT_OBJECT	BSET	#O_CLEAR,OBJ_FLAG0(A1)
		ADDQ.W	#1,OBJ_SHOT_AMT(A2)
		
		MOVE.W	#SFX_LAZERHIT_A,D0
		JMP	CSFX_ONESHOT_V

