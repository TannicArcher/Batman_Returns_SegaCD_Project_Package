PRINT_POLES	MOVEM.L	(A0)+,D3-D7
		SUB.L	D7,D5
		ADD.L	RCXPOS(A5),D4
		ADD.L	RCYPOS(A5),D5
		DIVS	D0,D3
		DIVS	D0,D4
		DIVS	D0,D5
		DIVS	D0,D6
		BNE.S	.xok
		MOVEQ	#1,D6
.xok		DIVS	D0,D7
		BNE.S	.yok
		MOVEQ	#1,D7
.yok		SWAP	D6
		MOVE.W	D7,D6
		SWAP	D6
		ADD.W	#256/2,D4
		ADD.W	#1+RSHEIGHT/2,D5
		MOVE.L	RCYMAX(A5),D7
		MOVE.L	RCXMAX(A5),D0

.loop		MOVEM.L	D0-D7/A0,-(SP)
		BSR	SCALE
	  	MOVEM.L	(SP)+,A0/D0-D7
		ADD.W	D3,D4
		BMI.S	.end
		CMP.W	#256,D4
		BMI.S	.loop
.end		RTS

