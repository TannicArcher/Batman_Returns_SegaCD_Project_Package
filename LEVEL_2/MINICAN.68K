MCBHIT_GROUND	JSR	O_CLEAR_S
		LEA	VIS_EXPD,A0
		MOVEQ	#0,D3
		JMP	VIS_SET_YZX

MCBHIT_OBJECT	JSR	O_CLEAR_S
		ADDQ.W	#6,OBJ_SHOT_AMT(A2)
		MOVE.L	#-$E00,D0
		MOVE.L	#-$100000,D1
		MOVE.L	#$E0000,D2
		TST.W	OBJ_ADDR4+2(A1)
		BPL.S	.set
		NEG.L	D0
		NEG.L	D1
		NEG.L	D2
.set		ADD.L	D0,OBJ_ADDR7(A2)
		ADD.L	D1,OBJ_SPD(A2)
		ADD.L	D2,OBJ_XINERTIA(A2)
		LEA	VIS_EXPD_MCH,A0
		MOVEQ	#0,D3
		JMP	VIS_SET_YZX

