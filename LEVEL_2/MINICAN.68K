; STANDARD ENEMY - LEVEL 2 - "Mini Cannon"

; --------------------------------------------------------------------------

MINI_SETUP_L	LEA	OBJ_MNCNR,A6
		JSR	O_OBJECT_SETUP	 
		BNE.S	.end
		MOVE.L	#LOGIC_MINI_A,OBJ_LOGIC_DATA(A1)
		MOVE.W	#$48,OBJ_LMSPD(A1)
		MOVE.W	#10,D7
		JSR	RANDOM_RANGE_U
		ADD.W	#10,D7
		MOVE.W	D7,OBJ_CNT0(A1)
		MOVE.L	#MINI_FIRE_R,OBJ_ADDR6(A1)

		MOVE.W	#SFX_EMINICAN_A,OBJ_FX0_SFXNUM(A1)

		MOVEQ	#0,D0
.end	  	RTS

MINI_SETUP_R	LEA	OBJ_MNCNL,A6
		JSR	O_OBJECT_SETUP	 
		BNE.S	.end
		MOVE.L	#LOGIC_MINI_B,OBJ_LOGIC_DATA(A1)
		MOVE.W	#$50,OBJ_LMSPD(A1)
		MOVE.W	#10,D7
		JSR	RANDOM_RANGE_U
		ADD.W	#10,D7
		MOVE.W	D7,OBJ_CNT0(A1)
		MOVE.L	#MINI_FIRE_L,OBJ_ADDR6(A1)
		
		MOVE.W	#SFX_EMINICAN_A,OBJ_FX0_SFXNUM(A1)
		
		MOVEQ	#0,D0
.end		RTS

; --------------------------------------------------------------------------

MINI_CLEAR	MOVE.L	OBJ_ADDR2(A1),A2
		JSR	O_DEALL_S
		JSR	O_CLEAR_S
		JSR	SFX3D_KILL
		JSR	CLEAR_DAMAGE
		JMP	SCADD_ENEMY

; --------------------------------------------------------------------------

MINI_MOVE	BTST	#O_CLEAR,OBJ_FLAG0(A1)
		BNE.S	MINI_CLEAR
		
		JSR	LOGIC_CTRL
		JSR	SFX3D_SPEED

		MOVE.L	OBJ_ADDR5(A1),A2
		BTST	#O_DEAD,OBJ_FLAG0(A2)
		BEQ.S	.dctrl
		BSET	#O_DEAD,OBJ_FLAG0(A1)
		MOVE.W	#$FFFF,OBJ_DTOTAL(A1)
.dctrl		JSR	DAMAGE_CTRL

		MOVE.L	OBJ_ADDR7(A1),D1
		MOVE.L	OBJ_YPOS(A1),D0
		BNE.S	.air_time
		CMP.L	#400,D1
		BPL	.air_skp
		ASL.L	#8,D1
		DIVS	#400,D1
		EXT.L	D1
.air_time	ADD.L	#800,D1
		ADD.L	D1,D0
		BMI.S	.air_skp
		MOVEQ	#0,D0
		NEG.L	D1
.air_skp	MOVE.L	D0,OBJ_YPOS(A1)
		MOVE.L	D1,OBJ_ADDR7(A1)

		TST.W	OBJ_CNT1(A1)
		BEQ.S	.ftimer
		SUBQ.W	#1,OBJ_CNT1(A1)
		BNE.S	.end	
		
		MOVE.W	#SFX_MCANFIRE_A,D0
		JSR	CSFX_ONESHOT_V
		
		MOVE.L	OBJ_ADDR6(A1),A0
		JMP	(A0)

.ftimer		CMP.W	#$300,OBJ_ZPOS(A1)
		BPL.S	.end
		SUBQ.W	#1,OBJ_CNT0(A1)
		BPL.S	.end
		MOVEQ	#20,D7
		JSR	RANDOM_RANGE_U
		ADD.W	#10,D7
		MOVE.W	D7,OBJ_CNT0(A1)
		MOVE.B	#6,OBJ_FLASH0(A1)
		MOVE.W	#10,OBJ_CNT1(A1)
		
.end		RTS

; --------------------------------------------------------------------------

MINI_PRINT	JSR	O_CAR_PRINT_S
		MOVE.L	OBJ_YPOS(A1),-(SP)
		BPL.S	.skp
		MOVE.B	OBJ_FLASH0(A1),D1
		MOVE.W	D1,-(SP)
		CLR.B	OBJ_FLASH0(A1)
		CLR.L	OBJ_YPOS(A1)
		LEA	OBJ_ADDR2(A1),A4
		LEA	OBJ_MC_SHADOW,A3
		JSR	O_CPS_ENTB
		MOVE.W	(SP)+,D1
		MOVE.B	D1,OBJ_FLASH0(A1)
.skp		MOVE.L	(SP)+,OBJ_YPOS(A1)
		JMP	SHOW_DAMAGE

; --------------------------------------------------------------------------

MINI_FIRE_R	JSR	O_CALC_PATCH
		LEA	OBJ_MCAN_POS_R,A0
		JSR	O_CALC_VIEW
		
		MOVEM.L	A0/A1,-(SP)
		MOVEM.L	(A0),D0/D1
		ADD.L	D0,OBJ_XPOS(A1)
		ADD.L	D1,OBJ_YPOS(A1)

		LEA	MCBALR_SET_DATA,A4
		LEA	OBJECT_DATA,A3

		MOVE.L	OBJ_SPD(A1),D0
		MOVE.W	OBJ_ZPOS(A1),D1
		MOVE.L	OBJ_XPOS(A1),D6
		ADD.L	#20*$100,D6
		MOVEQ	#0,D7

		JSR	O_THROW_TO_POS
		BNE.S	.end
		ADD.L	#$1000,OBJ_XPOS(A2)
		SUB.L	#$160000,OBJ_XINERTIA(A1)
		LEA	VIS_MCANFIRE_R,A0
		JSR	VIS_SET_XYZS

.end		MOVEM.L	(SP)+,A0/A1
		MOVEM.L	(A0),D0/D1
		SUB.L	D0,OBJ_XPOS(A1)
		SUB.L	D1,OBJ_YPOS(A1)
		RTS

; --------------------------------------------------------------------------

MINI_FIRE_L	JSR	O_CALC_PATCH
		LEA	OBJ_MCAN_POS_L,A0
		JSR	O_CALC_VIEW
		
		MOVEM.L	A0/A1,-(SP)
		MOVEM.L	(A0),D0/D1
		ADD.L	D0,OBJ_XPOS(A1)
		ADD.L	D1,OBJ_YPOS(A1)

		LEA	MCBALL_SET_DATA,A4
		LEA	OBJECT_DATA,A3

		MOVE.L	OBJ_SPD(A1),D0
		MOVE.W	OBJ_ZPOS(A1),D1
		MOVE.L	OBJ_XPOS(A1),D6
		SUB.L	#20*$100,D6
		MOVEQ	#0,D7

		JSR	O_THROW_TO_POS
		BNE.S	.end
		SUB.L	#$1000,OBJ_XPOS(A2)
		ADD.L	#$160000,OBJ_XINERTIA(A1)
		LEA	VIS_MCANFIRE_L,A0
		JSR	VIS_SET_XYZS

.end		MOVEM.L	(SP)+,A0/A1
		MOVEM.L	(A0),D0/D1
		SUB.L	D0,OBJ_XPOS(A1)
		SUB.L	D1,OBJ_YPOS(A1)
		RTS

