JET_SETUP	LEA	OBJ_JET,A6
		BSR	O_OBJECT_SETUP	 
		BNE.S	.end
		MOVE.W	#40,OBJ_CNT1(A1)
		MOVE.L	#LOGIC_JET,OBJ_LOGIC_DATA(A1)
		MOVE.W	#$50,OBJ_LMSPD(A1)

		MOVE.W	#SFX_EJETSKI_A,OBJ_FX0_SFXNUM(A1)

		MOVE.L	#-20*$100,OBJ_ADDR7(A1)
		MOVE.W	OBJECT_DATA+OBJ_SPD,D0
		MOVE.W	D0,D1
		ASR.W	#2,D1
		ADD.W	D1,D0
		MOVE.W	D0,OBJ_SPD(A1)
		MOVE.W	#20,OBJ_CNT7(A1)
		MOVE.W	#30*$80,OBJ_LZMIN(A1)
		MOVE.W	#32*$80,OBJ_LZMAX(A1)

		MOVEQ	#0,D0
.end		RTS

