JET_MOVE
		BTST	#O_CLEAR,OBJ_FLAG0(A1)
		BNE.S	JET_CLEAR
		
		BTST	#O_DEAD,OBJ_FLAG0(A1)
		BEQ.S	.go
		BSR	O_MOVER
		BRA.S	.go_skp
.go

		TST.W	OBJ_CNT7(A1)
		BEQ.S	.norm
		SUBQ.W	#1,OBJ_CNT7(A1)
		BSR	LOGIC_MOVER
		BRA.S	.go_skp
.norm
		BSR	LOGIC_CTRL
.go_skp
		JSR	SFX3D_SPEED	

		BSR	DAMAGE_CTRL

JET_AIR	  	MOVE.L	OBJ_ADDR7(A1),D1
		MOVE.L	OBJ_YPOS(A1),D0
		BNE.S	.air_time
		CMP.L	#400,D1
		BPL	.air_skp
		ASL.L	#8,D1
		DIVS	#400,D1
		EXT.L	D1
.air_time	ADD.L	#800,D1
		ADD.L	D1,D0
		BMI.S	.air_skp
		MOVEQ	#0,D0
		NEG.L	D1

		CMP.L	#-$A00,D1
		BPL.S	.air_skp

		MOVEM.L	D0/D1/A1,-(SP)
		MOVE.W	OBJ_ZPOS(A1),D0
		ADD.W	OBJ_ZSIZE(A1),D0
		MOVE.L	OBJ_XPOS(A1),D1
		MOVE.L	#0,D2
		LEA	VIS_JWSPLASH,A0
		JSR	VIS_SET_S
		MOVE.L	A1,A2
		MOVEM.L	(SP)+,D0/D1/A1
		BNE.S	.air_skp
      		
		MOVE.W	D1,D2			;Set size.
		ASR.W	#4,D2
		ADD.W	#$1D0,D2
		MOVE.W	D2,OBJ_CNT7(A2)

.air_skp	MOVE.L	D0,OBJ_YPOS(A1)
		MOVE.L	D1,OBJ_ADDR7(A1)

.fire_ctrl	MOVE.W	OBJ_CNT0(A1),D0
		BEQ.S	.fire_chk
		SUBQ.W	#1,D0
		MOVE.W	D0,OBJ_CNT0(A1)
		BEQ	CRMISS_SET
		SUBQ.W	#1,D0
		BNE.S	.end
.flash		LEA	VIS_CRFLASH,A0
		MOVE.L	OBJ_XPOS(A1),D1
		MOVE.L	OBJ_YPOS(A1),D2
		ADD.L 	#-30*$100,D2
		JMP	VIS_SET_ZS

.fire_chk	TST.W	OBJ_CNT1(A1)
		BEQ.S	.fdist
		SUBQ.W	#1,OBJ_CNT1(A1)
		RTS

.fdist		CMP.W	#$360,OBJ_ZPOS(A1)
		BMI.S	.end
		CMP.W	#$780,OBJ_ZPOS(A1)
		BPL.S	.end
		TST.L	OBJECT_DATA+OBJ_YPOS
		BNE.S	.end
	
		MOVE.B	#6,OBJ_FLASH0(A1)
		MOVE.W	#10,OBJ_CNT0(A1)	
		MOVE.W	#20,OBJ_CNT1(A1)
		SUBQ.W	#1,OBJ_CNT2(A1)
		BNE.S	.end
		MOVE.W	OBJ_CNT3(A1),OBJ_CNT2(A1)
		MOVE.W	OBJ_CNT4(A1),OBJ_CNT1(A1)
.end		RTS

